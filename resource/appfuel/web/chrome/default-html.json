{
	"name": "default-html",
	"type": "chrome",
	"tpl": "chrome/htmldoc.phtml",
	"layers": {
		"yui3": ["fw-global"],
		"appfuel": ["app-global"]
	},
	"html": {
		"attrs": {
			"lang": "eng",
			"manifest": "my-file"
		},
		"head": {
			"attrs": {"id":"head-id"},
			"title": "this is the default title",
			"base": {"href": "base-url.com"},
			"meta": [
				{
					"content":"text/html",
					"http-equiv": "Content-Type",
					"charset": "utf-8"
				},
				{
					"name": "framework",
					"content": "appfuel"
				}
			],
			"links": ["url1","url2"],
			"scripts": ["url1", "url2"],

			"body": {
				"scripts":["url1", "url2"]
			}
		}
	}
}
